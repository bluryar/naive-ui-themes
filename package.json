{
  "type": "module",
  "version": "0.2.2",
  "private": true,
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bluryar/naive-ui-themes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluryar/naive-ui-themes.git"
  },
  "bugs": {
    "url": "https://github.com/bluryar/naive-ui-themes/issues"
  },
  "keywords": [
    "libs",
    "vue3",
    "ts",
    "utils",
    "unocss"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:vue-ts\"",
    "dev:vue-ts": "nr -F ./playgrounds/vue-ts dev",
    "build": "nr -r build",
    "build:pkg": "nr -F ./packages/** build",
    "build:plg": "nr -F ./playgrounds/** build",
    "stub": "nr -r stub",
    "test": "vitest",
    "check:types": "tsc --noEmit",
    "clear:modules": "rimraf --glob ./{packages, playgrounds}/*/node_modules && rimraf ./node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "concurrently \"npm:preview:vue-ts\"",
    "preview:vue-ts": "nr -F ./playgrounds/vue-ts preview",
    "bump:patch": "nr -r bump:patch && npx bumpp patch --all --yes",
    "bump:minor": "nr -r bump:minor && npx bumpp minor --all --yes",
    "bump:major": "nr -r bump:major && npx bumpp major --all --yes",
    "release": "nr -r release",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.8",
    "@bluryar/commitlint-config": "^1.1.2",
    "@bluryar/eslint-config-vue": "^1.1.2",
    "@bluryar/shared": "^0.2.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/jsdom": "^21.1.2",
    "@types/lodash": "^4.14.198",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.15",
    "@types/tinycolor2": "^1.4.3",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "bumpp": "^9.2.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.7.1",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "cz-git": "^1.7.1",
    "dotenv": "^16.3.1",
    "esbuild": "0.19.2",
    "eslint": "^8.49.0",
    "fast-glob": "^3.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "pnpm": "^8.7.4",
    "taze": "^0.11.2",
    "tinycolor2": "^1.6.0",
    "tsx": "^3.12.8",
    "typescript": "~5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.55.7",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-inspect": "^0.7.38",
    "vitest": "^0.34.4",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.10",
    "vue-types": "^5.1.1"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx,cjs,mjs,cts,mts,yaml,md,yml}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
