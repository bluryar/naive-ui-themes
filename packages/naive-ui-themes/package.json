{
  "name": "@bluryar/naive-ui-themes",
  "type": "module",
  "version": "0.2.4",
  "private": false,
  "description": "âš¡ A plugin for naive-ui, unocss, and vueuse",
  "author": "bluryar <bluryarl@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bluryar/libsany/blob/main/packages/naive-ui-themes/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluryar/libsany.git"
  },
  "bugs": {
    "url": "https://github.com/bluryar/libsany/issues"
  },
  "keywords": [
    "naive-ui",
    "unocss",
    "vueuse",
    "theme",
    "themes",
    "vue3",
    "composables",
    "hooks"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./dist/preset-naive-themes": {
      "types": "./dist/preset-naive-themes.d.ts",
      "import": "./dist/preset-naive-themes.mjs"
    },
    "./dist/naive-multi-theme": {
      "types": "./dist/naive-multi-theme.d.ts",
      "import": "./dist/naive-multi-theme.mjs"
    },
    "./dist/module-loader": {
      "types": "./dist/module-loader.d.ts",
      "import": "./dist/module-loader.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "bump:major": "npx bumpp major --no-commit --no-tag --no-push --yes",
    "bump:minor": "npx bumpp minor --no-commit --no-tag --no-push --yes",
    "bump:patch": "npx bumpp patch --no-commit --no-tag --no-push --yes",
    "release": "npm publish",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "@vueuse/core": ">=10.0.0",
    "naive-ui": ">=2.0.0",
    "unocss": ">=0.50.0",
    "vite": ">=4.0.0",
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@unocss/preset-mini": "^0.55.7",
    "esbuild": "0.19.2",
    "fast-glob": "^3.3.1",
    "lodash": "^4.17.21",
    "mlly": "^1.4.2",
    "tinycolor2": "^1.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
